    <string name="createTable">CREATE TABLE IF NOT EXISTS STRALARM (
        NUM INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
        NAME TEXT,
        START_H INTEGER,
        START_M INTEGER,
        END_H INTEGER,
        END_M INTEGER,
        INTERVAL INTEGER,
        SETTINGS INTEGER
        )</string>
    <string name="insertAlarm">insert into STRALARM(NAME , START_H , START_M , END_H , END_M , INTERVAL, SETTINGS)  values($name, $sh, $sm, $eh, $em, $intvl, $settings)</string>
    <string name="selectLatest">select * from STRALARM order by NUM desc limit 1</string>

